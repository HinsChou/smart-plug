<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <ViewFlipper 
        android:id="@+id/renwu_menu_body"
        android:background="#FFFFFFFF" 
        android:paddingBottom="55.0dip" 
        android:layout_width="fill_parent" 
        android:layout_height="fill_parent" />
    
    <LinearLayout 
        android:layout_gravity="bottom" 
        android:gravity="bottom|center"
        android:id="@+id/renwu_menu_bg" 
        android:background="@drawable/set_all_bg" 
        android:paddingTop="2.0dip" 
        android:paddingBottom="4.0dip"
        android:layout_width="fill_parent" 
        android:layout_height="70.0dip">
        
        <LinearLayout 
            android:gravity="center" 
            android:orientation="vertical" 
            android:id="@+id/renwu_menu_socket_layout" 
            android:layout_width="fill_parent" 
            android:layout_height="fill_parent" 
            android:layout_weight="1.0">
            
            <ImageView 
                android:id="@+id/renwu_menu_socket_img" 
                android:background="@drawable/mainsocket" 
                android:layout_width="34.0dip" 
                android:layout_height="34.0dip" 
                android:layout_marginTop="4.0dip" />
            <TextView 
                android:textSize="11.0sp" 
                android:textColor="#ff000000" 
                android:layout_width="wrap_content" 
                android:layout_height="wrap_content" 
                android:layout_margin="2.0dip" 
                android:text="@string/renwu_socket" />
        </LinearLayout>
        
        <LinearLayout 
            android:gravity="center" 
            android:orientation="vertical" 
            android:id="@+id/renwu_menu_switch_layout" 
            android:layout_width="fill_parent" 
            android:layout_height="fill_parent" 
            android:layout_weight="1.0">
            
            <ImageView android:id="@+id/renwu_menu_switch_img" 
                android:background="@drawable/mainswitch" 
                android:layout_width="34.0dip" 
                android:layout_height="34.0dip" 
                android:layout_marginTop="4.0dip" />
            <TextView android:textSize="12.0sp" 
                android:textColor="#ff000000" 
                android:layout_width="wrap_content" 
                android:layout_height="wrap_content" 
                android:layout_margin="2.0dip" 
                android:text="@string/renwu_switch" />
        </LinearLayout>
        <LinearLayout android:gravity="center" 
            android:orientation="vertical" 
            android:id="@+id/renwu_menu_control_layout" 
            android:layout_width="fill_parent" 
            android:layout_height="fill_parent" 
            android:layout_weight="1.0">
         
            <ImageView android:id="@+id/renwu_menu_control_img" 
                android:background="@drawable/maincontrol" 
                android:layout_width="34.0dip" 
                android:layout_height="34.0dip" 
                android:layout_marginTop="4.0dip" />
            <TextView android:textSize="12.0sp" 
                android:textColor="#ff000000" 
                android:layout_width="wrap_content" 
                android:layout_height="wrap_content" 
                android:layout_margin="2.0dip" 
                android:text="@string/renwu_control" />
        </LinearLayout>
        <LinearLayout android:gravity="center" 
            android:orientation="vertical" 
            android:id="@+id/renwu_menu_help_layout" 
            android:layout_width="fill_parent" 
            android:layout_height="fill_parent" 
            android:layout_weight="1.0">
         
            <ImageView android:id="@+id/renwu_menu_help_img" 
                android:background="@drawable/more" 
                android:layout_width="34.0dip" 
                android:layout_height="34.0dip" 
                android:layout_marginTop="4.0dip" />
            <TextView android:textSize="12.0sp" 
                android:textColor="#ff000000" 
                android:layout_width="wrap_content" 
                android:layout_height="wrap_content" 
                android:layout_margin="2.0dip" 
                android:text="@string/renwu_more" />
        </LinearLayout>
    </LinearLayout>
</merge>